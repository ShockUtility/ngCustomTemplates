{
  "$schema": "http://json-schema.org/schema",
  "id": "HelloSchematics",
  "title": "기초코드 템플릿",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "클래스 또는 파일 이름",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "클래스명 입력 (ex> BsSalesCountryCode) : "
    },
    "project": {
      "type": "string",
      "description": "Angular CLI workspace project"
    }
  },
  "required": ["name"]
}
